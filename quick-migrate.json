{
  "defaults": {
    "largeFileThresholdMB": 1024,
    "robocopyThreads": 8,
    "targetStrategy": "unified",
    "unifiedTargetRoot": "D:\\Migrated",
    "restoreKeepTarget": false
  },
  "profiles": [
    {
      "id": "NovaStudio",
      "name": "智玲同学",
      "locator": {
        "type": "registryDisplayIcon",
        "hive": "HKCU",
        "keyPath": "Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\NovaStudio",
        "valueName": "DisplayIcon"
      },
      "items": [
        {
          "displayName": "comfyui模型目录",
          "relativePath": "comfyui\\models",
          "enabled": true
        },
        {
          "displayName": "comfyui输出目录",
          "relativePath": "comfyui\\output",
          "enabled": true
        }
      ]
    },
    {
      "id": "linglong",
      "name": "模型管理",
      "locator": {
        "type": "absolutePath",
        "path": "C:\\LingLong"
      },
      "items": [
        { "displayName": "模型管理", "relativePath": ".llmodels", "enabled": true },
        { "displayName": "测试", "relativePath": "test", "enabled": true }
      ]
    }
  ],
  "standaloneSources": [
    {
      "displayName": "自定义资源库",
      "absolutePath": "D:\\MyResources",
      "enabled": true
    }
  ]
}


